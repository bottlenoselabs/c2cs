{
    "Logging": {
        "Console": {
            "LogLevel": {
                "Default": "Debug",
                "Microsoft.Hosting.Lifetime": "Information",
                "C2CS": "Debug"
            },
            "FormatterOptions": {
                "ColorBehavior": "Disabled",
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "yyyy-dd-MM HH:mm:ss ",
                "UseUtcTimestamp": true
            }
        }
    }
}