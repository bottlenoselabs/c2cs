{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "C2CS": "Information"
      },
      "FormatterOptions": {
        "ColorBehavior": "Disabled",
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-dd-MM HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  }
}
