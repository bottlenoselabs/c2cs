        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>C2CS.Feature.BuildLibraryC</RootNamespace>
        <NoWarn>$(NoWarn);CA1724</NoWarn>
    </PropertyGroup>

    <!-- Project references -->
    <ItemGroup>
        <ProjectReference Include="../../infrastructure/C2CS.Common/C2CS.Common.csproj" />
